{
  "name": "enhanced-ms",
  "type": "module",
  "version": "5.0.0",
  "packageManager": "pnpm@10.26.1",
  "scripts": {
    "check": "tsc --noEmit",
    "lint": "biome check",
    "format": "biome check --write",
    "test": "vitest --run",
    "build": "tsdown"
  },
  "files": ["dist", "!dist/**/*.map"],
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./locales/*": {
      "types": "./dist/locales/*.d.mts",
      "default": "./dist/locales/*.mjs"
    }
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "tsdown": "^0.18.2",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16"
  }
}
